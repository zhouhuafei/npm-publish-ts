{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@root/*": [
        "./*"
      ],
      "@src/*": [
        "./src/*"
      ],
      "@/*": [
        "./src/*"
      ]
    },
    "strict": true,
    "noImplicitAny": false,
    "esModuleInterop": true,
    "resolveJsonModule": true
  }
}
